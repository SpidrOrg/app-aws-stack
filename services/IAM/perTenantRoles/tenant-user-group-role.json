{
  "roleName": ":TENANT_ID:-user-group-role",
  "roleConfig": {
    "policies": [
      ":TENANT_ID:-datascientist-policy"
    ],
    "description": "",
    "trustRelationship": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Principal": {
            "AWS": [
              "arn:aws:iam::123456789012:user/root"
            ]
          },
          "Action": "sts:AssumeRole",
          "Condition": {
            "Bool": {
              "aws:MultiFactorAuthPresent": "true"
            }
          }
        }
      ]
    }
  }
}
